type Query {
	draft: Draft
	isAlive: Boolean
	isClientRunning: Boolean
	toggleClientRunning: Boolean
}

type Draft {
	blue: Team
	red: Team
}

type Team {
	picks: [Character]
	banns: [Character]
	combos: [Character]
	openRoles: [Role]
}

enum Role {
	TOP
	JUNGLE
	MID
	BOTTOM
	SUPPORT
}

type Character {
	name: String
	priorities: [Character]
	priority: Int
}